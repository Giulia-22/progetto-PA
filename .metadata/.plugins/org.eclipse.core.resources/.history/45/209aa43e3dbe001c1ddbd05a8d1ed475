
#include "Comparatori.h"

Comparatori::Comparatori(){}

static bool Comparatori::compareByCategoria(paziente_ref &a, paziente_ref &b) {
	return a->getCategoria() < b->getCategoria();
}

static bool Comparatori::compareBySpecializzazione(dottore_ref &a, dottore_ref &b) {
	return a->getSpecializzazione() < b->getSpecializzazione();
}

virtual bool Comparatori::compareByID(paziente_ref &a, paziente_ref &b) {
	return a->getIdPaz() < b->getIdPaz();
}

virtual bool Comparatori::compareByID(dottore_ref &a, dottore_ref &b) {
	return a->getIdDott() < b->getIdDott();
}

Comparatori::~Comparatori(){}

