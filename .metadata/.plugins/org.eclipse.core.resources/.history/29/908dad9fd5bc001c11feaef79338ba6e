
#include "metodi.h"

void stampa(vector<Persona> p){
	for(vector<Persona>::iterator i = p.begin(); i != p.end(); ++i){
		//cout<<"Nome: "<<i->nome<<endl;
		//cout<<"Cognome: "<<i->cognome<<endl;
		cout<<"Anno di nascita: "<<i->annonascita<<endl;
		cout<<"Codifce fiscale: "<<i->cf<<endl<<endl;
	}
}

