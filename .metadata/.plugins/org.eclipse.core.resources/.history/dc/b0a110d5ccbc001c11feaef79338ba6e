#include <iostream>
#include <vector>
#include <memory>

#include "Persona.h"

using namespace std;

int main() {
	Persona* p1 = new Persona(1999, (char*)"1234567890udtqq", (char*)"1", (char*)"A");
	Persona* p2 = new Persona(1999, (char*)"1234567890udtqq", (char*)"2", (char*)"A");
	Persona* p3 = new Persona(1999, (char*)"1234567890udtqq", (char*)"3", (char*)"A");
	Persona* p4 = new Persona(1999, (char*)"1234567890udtqq", (char*)"4", (char*)"A");
	p1->stampa();
	vector<Persona> vp1(4);
	vp1[0] = (Persona&)p1
	vp1[1] = p2;
	vp1[2] = p3;
	vp1[3] = p4;
	return 0;
}
